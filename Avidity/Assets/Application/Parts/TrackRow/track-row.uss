TrackRow {
    padding-top: 10px;
    padding-bottom: 10px;
    border-bottom-color: rgb(192, 192, 192);
    border-bottom-width: 0.5px;
}
TrackRow:hover {
    background-color: rgba(0, 0, 0, 0.5);
}

Label {
    padding-top: 0px;
    padding-bottom: 0px;
}

#track-name {
    color: white;
    font-size: 30px;
}

#artist-name {
    color: rgb(192, 192, 192);
    font-size: 25px;
}

#track-duration {
    color: rgb(192, 192, 192);
    font-size: 25px;
}
